{
  "name": "node-nim-compiler",
  "version": "5.0.4",
  "description": "A Node.js interface to the Nim compiler binaries.",
  "main": "dist/index.js",
  "scripts": {
    "prepack": "npm run build",
    "build": "rm -rf dist/ && tsc",
