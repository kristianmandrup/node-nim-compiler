  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/nim-community/node-nim-compiler/issues"
  },
  "homepage": "https://github.com/nim-community/node-nim-compiler",
  "dependencies": {
    "cross-spawn": "^7.0.1",
    "lodash": "^4.17.15",
    "temp": "^0.9.1"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.12.28",
    "chai": "^4.2.0",
    "glob": "^7.1.6",
    "mocha": "^10.2.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.5.3"
  }
